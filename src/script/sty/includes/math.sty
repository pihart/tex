% Evenly spaced matrix
\newcommand\sqmatrix[2][c]{%
    \fixTABwidth{T}%
    \setbox0=\hbox{$\tabbedCenterstack{#2}$}%
    \setstackgap{L}{\dimexpr\maxTAB@width+\tabbed@gap}%
    \tabbedCenterstack[#1]{#2}%
}

